@charset "UTF-8";
@import url('https://fonts.googleapis.com/css?family=Poppins:200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i|Raleway:100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet');
* {
    margin: 0;
    padding: 0;
}
body {
    background-color: #ffffff;
    color: #808285;
    font-family: 'Poppins', sans-serif;
    font-size: 14px;
    font-weight: normal;
}
body h1, body h2, body h3, body h4, body h5, body h6 {
    color: #363636;
    font-family: 'Poppins', sans-serif;
    font-style: normal;
    line-height: 1.5;
    letter-spacing: 1.1px;
    margin: 0 0 8px;
}
h1 {
    font-size: 26px;
}
h2 {
    font-size: 24px;
}
h3 {
    font-size: 22px;
}
h4 {
    font-size: 20px;
}
h5 {
    font-size: 18px;
}
h6 {
    font-size: 16px;
}
h1 a, h2 a, h3 a, h4 a, h5 a, h6 a {
    color: #363636;
}
p a {
    color: #363636;
}
p {
    line-height: 24px;
    letter-spacing: 1.1px;
    margin-bottom: 10px;
}
a {
    color: #363636;
    text-decoration: none;
}
a:hover, a:focus, a:active {
    outline: none;
}
ul {
    margin-bottom: 0;
}
figure {
    margin-bottom: 6px;
    position: relative;
}
img {
    height: auto;
    max-width: 100%;
}
strong {
    color: #555555;
}
iframe {
    border: none;
    float: left;
    width: 100%;
}
.wp-caption, .wp-caption-text, .gallery-caption, .bypostauthor {
}
.wp-caption.alignleft, .wp-caption.alignnone {
    width: 100%;
}
select {
    background-color: #f0eef0;
    border: 1px solid #dfdfdf;
    color: #666666;
    cursor: pointer;
    display: inline-block;
    font-size: 14px;
    font-weight: 300;
    height: 40px;
    padding: 8px 12px;
    width: 100%;
}
label {
    color: #6c8391;
    display: block;
    font-weight: 400;
    margin-bottom: 10px;
}
button {
    border: none;
}
textarea, input[type="text"], input[type="password"], input[type="datetime"], input[type="datetime-local"], input[type="date"], input[type="month"], input[type="time"], input[type="week"], input[type="number"], input[type="email"], input[type="url"], input[type="search"], input[type="tel"], input[type="color"], .uneditable-input {
    background-color: #f2f2f2;
    border: none;
    box-sizing: border-box;
    color: #aaa9a9;
    display: inline-block;
    font-size: 13px;
    height: 30px;
    line-height: 20px;
    outline: none;
    padding: 4px 6px;
    vertical-align: middle;
    box-shadow: none;
}
input[type="submit"] {
    outline: none;
}
form p {
    float: left;
    position: relative;
}
form p span i {
    color: #474747;
    left: 16px;
    position: absolute;
    top: 13px;
}
option {
    padding: 5px 15px;
}
li {
    line-height: 28px;
    list-style: inside none disc;list-style: none;
}
ul ul {
    margin-left: 20px;
}
ol {
    float: none;
    list-style: outside none decimal;
    padding-left: 15px;
}
ol ol {
    margin-left: 20px;
}
ol li {
    list-style: outside none decimal;
    width: 100%;
}
table {
    background-color: transparent;
    max-width: 100%;
}
th {
    text-align: left;
}
table {
    margin-bottom: 15px;
    width: 100%;
}
table > thead > tr > th, table > tbody > tr > th, table > tfoot > tr > th, table > thead > tr > td, table > tbody > tr > td, table > tfoot > tr > td {
    border: 1px solid #dddddd;
    line-height: 1.5;
    padding-left: 3px;
    text-align: center;
    vertical-align: top;
}
table thead tr th {
    border-top: 1px solid #dddddd;
    text-align: center;
    text-transform: capitalize;
}
.table > caption + thead > tr:first-child > th, .table > colgroup + thead > tr:first-child > th, .table > thead:first-child > tr:first-child > th, .table > caption + thead > tr:first-child > td, .table > colgroup + thead > tr:first-child > td, .table > thead:first-child > tr:first-child > td {
    border-top: 1px solid #dddddd;
}
table > thead > tr > th {
    border-bottom: 2px solid #dddddd;
    vertical-align: bottom;
}
table > caption + thead > tr:first-child > th, table > colgroup + thead > tr:first-child > th, table > thead:first-child > tr:first-child > th, table > caption + thead > tr:first-child > td, table > colgroup + thead > tr:first-child > td, table > thead:first-child > tr:first-child > td {
    border-bottom: 0 none;
}
table > tbody + tbody {
    border-top: 2px solid #dddddd;
}
.table > thead > tr > th, .table > tbody > tr > th, .table > tfoot > tr > th, .table > thead > tr > td, .table > tbody > tr > td, .table > tfoot > tr > td {
    padding: 6px 10px;
}
p ins {
    color: #999;
}
dl dd   { margin-left: 20px; }
address { font-style: italic; }
.header-nav .collapse { display: block; }
.navbar-toggler { display: none; }
.ccg-main-section {
    float: left;
    width: 100%;
    position: relative;
}
.ccg-main-content       { padding: 80px 0px 60px 0px; }
.ccg-right-section  { float: right; }
.ccg-left-section       { float: left; }
.ccg-main-wrapper,.ccg-main-content,
.ccg-main-header,.ccg-navigation-section {
    float: left;
    width: 100%;
}
/* Header */
#ccg-header {
    float: left;
    width: 100%;
    z-index: 999;
}
.ccg-header-one {
    background-color: #92d050;
    position: relative;
    padding: 0px 0px 8px;
}
.ccg-header-one .container-fluid { padding: 0px 50px; }
.logo {
    display: inline-block;
    margin: 0px 0 -65px;
    background: #92d050;
    padding: 15px;
    border-radius: 100%;
}
.header-btn {
    background-color: #da251c;
    color: #ffffff;
    float: right;
    border: 2px solid #da251c;
    padding: 8px 20px;
    margin: 17px 0 0 10px;
    border-radius: 5px;
    transition: all 0.4s ease-in-out;
}
.header-btn:hover { color: #fff; background-color: transparent; border-color: #ffffff; }
/* Banner */
.ccg-banner,.ccg-banner-slide {
    float: left;
    width: 100%;
    height: 100vh;
    position: relative;
}
.ccg-banner-layer {
    float: left;
    width: 100%;
    height: 100vh;
    position: relative;
    background-position: center 89px;
    background-size: cover;
}
.ccg-banner-caption h1 {
    font-size: 48px;
    font-weight: bold;
    color: #ffffff;
    text-transform: uppercase;
    display: inline-block;
    width: 65%;
}
/*.ccg-banner-caption {
    position: absolute;
    left: 0px;
    top: 50%;
    width: 100%;
    text-align: center;
    margin-top: -70px;
}
.blue-transparent {
    position: absolute;
    left: 0px;
    top: 0px;
    width: 100%;
    height: 100%;
    background-color: rgba(33,47,71,0.30);
}*/
/* Navigation */
.header-nav {
    float: left;
    width: 100%;
    padding: 7px 0px 0px;
}
.navbar {
    float: right;
    padding: 0px;
}
.navbar,.navbar-nav {
    display: inline-block;
}
.navbar-nav li {
    float: left;
    margin: 0px 15px;
}
.navbar-nav > li > a {
    position: relative;
    font-size: 18px;
    font-weight: 500;
    padding: 23px 0px 18px !important;
}
.navbar-nav > li.dropdown > a > small {
    font-size: 15px;
    font-weight: 500;
    margin: 0 0 0 5px;
}
.navbar-nav > li.dropdown > a { padding: 15px 0px 15px !important; }
.navbar-nav > li.dropdown { margin: 0px 12px 0 9px; }
.navbar-nav > li > a:before {
    content: '';
    position: absolute;
    left: 50%;
    bottom: 13px;
    width: 0%;
    height: 2px;
    background-color: #000;
    transition: all 0.4s ease-in-out;
}
.navbar-nav > li.dropdown > a:before { display: none; }
.navbar-nav > li.dropdown > a.dropdown-toggle::after { margin-left: 0px; vertical-align: middle; }
.navbar-nav > li.active > a:before { left: 0px; width: 100%; }
.navbar-nav > li > a img { border-radius: 100%; }
/*Content*/
.ccg-fancy-title {
    float: left;
    width: 100%;
}
/*Fancy Title*/
.ccg-fancy-title { margin: 0px 0px 40px;}
.ccg-fancy-title h2 {
    margin: 0px 0px 0px;
    display: inline-block;
    font-size: 24px;
    position: relative;
    padding: 0px 0px 17px;
    line-height: 1;
}
.ccg-fancy-title h2:before {
    content: '';
    position: absolute;
    left: 0px;
    bottom: 0px;
    width: 80px;
    height: 2px;
    background-color: #da251c;
}
.ccg-fancy-title h2:after {
    content: '';
    position: absolute;
    left: 90px;
    bottom: -1px;
    width: 5px;
    height: 5px;
    border-radius: 100%;
    background-color: #da251c;
}
.ccg-fancy-title p {
    margin: 0px;
    line-height: 26px;
}
/*Fancy Title*/

/*Tour Modern*/
.ccg-tour,.ccg-tour-modern figure,.ccg-tour-modern figure > a,.ccg-tour-modern figure a img,
.ccg-tour-modern-text {
    float: left;
    width: 100%;
}
.ccg-tour > ul > li {
    list-style: none;
    float: left;
    width: 33.333%;
    margin: -1px 0px 0px;
}
.ccg-tour-modern figure > a {
    position: relative;
    -webkit-transition: all 0.4s ease-in-out;
       -moz-transition: all 0.4s ease-in-out;
        -ms-transition: all 0.4s ease-in-out;
         -o-transition: all 0.4s ease-in-out;
            transition: all 0.4s ease-in-out;
}
.ccg-tour-modern figure > a:before {
    content: '';
    position: absolute;
    left: 0px;
    top: 0px;
    width: 100%;
    height: 100%;
    background-color: #152c44;
    opacity: 0.4;
    -webkit-transition: all 0.4s ease-in-out;
       -moz-transition: all 0.4s ease-in-out;
        -ms-transition: all 0.4s ease-in-out;
         -o-transition: all 0.4s ease-in-out;
            transition: all 0.4s ease-in-out;
}
.ccg-tour-modern figure > a:after {
    content: '';
    position: absolute;
    left: 0px;
    top: 0px;
    width: 100%;
    height: 0%;
    background-color: #000;
    opacity: 0;
    -webkit-transition: all 0.4s ease-in-out;
       -moz-transition: all 0.4s ease-in-out;
        -ms-transition: all 0.4s ease-in-out;
         -o-transition: all 0.4s ease-in-out;
            transition: all 0.4s ease-in-out;
}
.ccg-tour-modern ul li:hover figure > a:after {
    opacity: 0.7;
    height: 100%;
}
.ccg-tour-modern figure > a i {
    position: absolute;
    left: 50%;
    bottom: 0%;
    width: 40px;
    height: 40px;
    color: #ffffff;
    font-size: 14px;
    text-align: center;
    border-radius: 100%;
    padding: 13px 0px 0px;
    margin: 0px 0px 2px -25px;
    opacity: 0;
    z-index: 1;
    background-color: #da251c;
    -webkit-transition: all 0.4s ease-in-out;
       -moz-transition: all 0.4s ease-in-out;
        -ms-transition: all 0.4s ease-in-out;
         -o-transition: all 0.4s ease-in-out;
            transition: all 0.4s ease-in-out;
}
.ccg-tour-modern ul li:hover figure > a i {
    bottom: 50%;
    opacity: 1;
}
.ccg-tour-modern figure {
    margin: 0px;
    padding: 0px 0px 0px;
    overflow: hidden;
}
.ccg-tour-modern figure > span {
    position: absolute;
    right: 20px;
    top: 20px;
    font-size: 12px;
    padding: 5px 13px;
    color: #ffffff;
    background-color: #9351de;
    line-height: 1;
    border-radius: 3px;
}
.ccg-tour-modern-text {
    background-color: #f5f5f5;
    padding: 10px 0px 10px 10px;
    opacity: 1;
    position: absolute;
    left: 0px;
    bottom: -100px;
    -webkit-transition: all 0.4s ease-in-out;
       -moz-transition: all 0.4s ease-in-out;
        -ms-transition: all 0.4s ease-in-out;
         -o-transition: all 0.4s ease-in-out;
            transition: all 0.4s ease-in-out;
}
.ccg-tour-modern ul li:hover .ccg-tour-modern-text {
    bottom: 0px;
}
.ccg-tour-modern-text span {
    font-size: 13px;
    font-weight: 300;
}
.ccg-tour-modern-text h5 {
    margin: 0px 0px 5px;
    line-height: 1.3;
    float: left;
    width: 100%;
    font-size: 14px;
}
.ccg-tour-option {
    float: left;
    width: 100%;
    margin: 0px 0px 15px;
}
.ccg-tour-option li {
    float: left;
    list-style: none;
    margin: 0px 20px 0px 0px;
}
.ccg-tour-option li a {
    font-size: 12px;
    color: #808285;
}
.ccg-tour-option li a i { margin: 0px 10px 0px 0px;}
.ccg-tour-modern-text p {
    margin: 0px 0px 8px;
    line-height: 26px;
    letter-spacing: 0.5px;
}
/*Content Post*/
.post-transpernt {
    position: absolute;
    left: 0px;
    top: 0px;
    width: 100%;
    height: 100%;
    background-color: #253556;
    opacity: 0.4;
}
/*Blog Modern*/
.ccg-blog,.ccg-blog-modern figure,.ccg-blog-modern figure a,.ccg-blog-modern figure a img,
.ccg-blog-modern-text {
    float: left;
    width: 100%;
}
.ccg-blog > ul > li {
    list-style: none;
    margin: 0px 0px 30px;
}
.ccg-blog-modern figure { margin: 0px;}
.ccg-blog-modern figure a {
    position: relative;
    overflow: hidden;
}
.ccg-blog-modern figure a:before {
    content: '';
    position: absolute;
    left: 0px;
    bottom: 0px;
    height: 100%;
    width: 100%;
    background-color: #152c44;
    opacity: 0.3;
    -webkit-transition: all 0.4s ease-in-out;
       -moz-transition: all 0.4s ease-in-out;
        -ms-transition: all 0.4s ease-in-out;
         -o-transition: all 0.4s ease-in-out;
            transition: all 0.4s ease-in-out;
}
.ccg-blog-modern figure a:after {
    content: '';
    position: absolute;
    left: 0px;
    bottom: 0px;
    height: 0%;
    width: 100%;
    background-color: #000;
    opacity: 0;
    -webkit-transition: all 0.4s ease-in-out;
       -moz-transition: all 0.4s ease-in-out;
        -ms-transition: all 0.4s ease-in-out;
         -o-transition: all 0.4s ease-in-out;
            transition: all 0.4s ease-in-out;
}
.ccg-blog-modern ul li:hover figure a:after {
    opacity: 0.8;
    height: 100%;
}
.ccg-blog-modern figure a i {
    position: absolute;
    left: 50%;
    top: 0%;
    border-radius: 100%;
    font-size: 16px;
    text-align: center;
    color: #ffffff;
    background-color: #da251c;
    z-index: 1;
    width: 60px;
    height: 60px;
    padding: 22px 0px 0px;
    margin: -30px 0px 0px -30px;
    opacity: 0;
    -webkit-transition: all 0.4s ease-in-out;
       -moz-transition: all 0.4s ease-in-out;
        -ms-transition: all 0.4s ease-in-out;
         -o-transition: all 0.4s ease-in-out;
            transition: all 0.4s ease-in-out;
}
.ccg-blog-modern ul li:hover figure a i {
    opacity: 1;
    top: 50%;
}
.ccg-blog-modern-text { padding: 30px 0px 0px 0px;}
.ccg-blog-modern-text time {
    width: 65px;
    height: 60px;
    float: left;
    color: #ffffff;
    background-color: #da251c;
    font-size: 18px;
    font-family: 'Raleway', sans-serif;
    text-transform: uppercase;
    text-align: center;
    font-weight: 500;
    padding: 10px 12px 0px;
    line-height: 1.2;
    position: relative;
}
.ccg-blog-modern-text time:before {
    content: '';
    position: absolute;
    left: 0px;
    bottom: -7px;
    width: 55px;
    background-color: #da251c;
    height: 2px;
    -webkit-transition: all 0.4s ease-in-out;
       -moz-transition: all 0.4s ease-in-out;
        -ms-transition: all 0.4s ease-in-out;
         -o-transition: all 0.4s ease-in-out;
            transition: all 0.4s ease-in-out;
}
.ccg-blog-modern-text time ~ h5 { margin: 0px 0px 3px; padding: 2px 0px 0px 85px;}
.ccg-blog-modern-text time ~ .ccg-blog-option {
    margin: 0px;
    padding: 0px 0px 0px 85px;
}
.ccg-blog-option li {
    list-style: none;
    float: left;
    font-size: 12px;
    margin: 0px 21px 0px 0px;
    position: relative;
}
.ccg-blog-option li:before {
    content: '';
    position: absolute;
    right: -11px;
    top: 8px;
    width: 2px;
    height: 10px;
    background-color: #b9babc;
    -webkit-transition: all 0.4s ease-in-out;
       -moz-transition: all 0.4s ease-in-out;
        -ms-transition: all 0.4s ease-in-out;
         -o-transition: all 0.4s ease-in-out;
            transition: all 0.4s ease-in-out;
}
.ccg-blog-option li:last-child:before { content: none;}
.ccg-blog-option li a {
    color: #808285;
    font-family: 'Raleway', sans-serif;
}
.ccg-blog-option li a i { margin: 0px 5px 0px 0px;}
/*Footer*/
.ccg-footer-one,.ccg-footer-newsletter {
    float: left;
    width: 100%;
}
.ccg-footer-newsletter {
    position: relative;
    padding: 25px 0px 23px;
    background: url("images/newsletter-transpernt.png");
}
.newsletter-transpernt {
    position: absolute;
    left: 0px;
    top: 0px;
    width: 100%;
    height: 100%;
    background-color: #363636;
    opacity: 0.85;
}
.ccg-newsletter-text { float: left;}
.ccg-newsletter-text h2 {
    margin: 0px;
    font-size: 30px;
    color: #ffffff;
    line-height: 1.4;
}
.ccg-newsletter-text p {
    margin: 0px;
    color: #ffffff;
}
.ccg-footer-newsletter form {
    float: right;
    width: 457px;
    position: relative;
    margin: 11px 0px 0px;
}
.ccg-footer-newsletter form input[type="email"] {
    float: left;
    width: 100%;
    background-color: #3f3f3f;
    border: 1px solid #717171;
    height: 44px;
    font-size: 12px;
    color: #909090;
    padding-left: 19px;
    padding-right: 135px;
}
.ccg-footer-newsletter form input[type="submit"] {
    position: absolute;
    right: 0px;
    top: 0px;
    color: #ffffff;
    border: none;
    font-weight: 500;
    letter-spacing: 1px;
    padding: 13px 23px 11px;
    background-color: #da251c;
}
/*Footer Widget*/
.footer-logo {
    float: left;
    margin: 0px 0px 10px;
    width: 100%;
}
.ccg-footer-widget {
    float: left;
    width: 100%;
    background-image: url(images/content-post-img.jpg);
    background-size: cover;
    background-attachment: fixed;
    position: relative;
    padding: 60px 0px 0px;
}
.ccg-footer-widget .widget { margin: 0px 0px 30px;}
/*widget_social_info*/
.widget_text p {
    color: #fff;
    letter-spacing: 0.8px;
    margin: 0px 0px 14px;
    line-height: 26px;
}
.ccg-footer-social {
    float: left;
    width: 100%;
    margin: 15px 0px 0px;
}
.ccg-footer-social li {
    list-style: none;
    display: inline-block;
    margin: 0px 10px 10px 0px;
}
.ccg-footer-social li a {
    width: 115px;
    height: 35px;
    float: left;
    color: #fff;
    background-color: #da251c;
    text-align: center;
    padding: 11px 0px 0px;
    border-radius: 3px;
    -webkit-transition: all 0.4s ease-in-out;
       -moz-transition: all 0.4s ease-in-out;
        -ms-transition: all 0.4s ease-in-out;
         -o-transition: all 0.4s ease-in-out;
            transition: all 0.4s ease-in-out;
}
.ccg-footer-social li a span { font-family: 'Poppins', sans-serif !important; }
.ccg-footer-social li:hover a {
    background-color: #92d050;
    color: #ffffff;
}
/*ccg-footer-title*/
.ccg-footer-title {
    float: left;
    width: 100%;
    margin: 0px 0px 20px;
}
.ccg-footer-title h2 {
    margin: 0px;
    font-size: 20px;
    color: #ffffff;
    padding: 0px 0px 13px;
    display: inline-block;
    position: relative;
    line-height: 1; 
}
.ccg-footer-title h2:before {
    content: '';
    position: absolute;
    left: 0px;
    bottom: 0px;
    width: 60px;
    height: 2px;
    background-color: #da251c;
}
.ccg-footer-title h2:after {
    content: '';
    position: absolute;
    left: 70px;
    bottom: -1px;
    width: 5px;
    height: 5px;
    border-radius: 100%;
    background-color: #da251c;
}
/*widget_twitter_feeds*/
.widget_twitter_feeds ul li {
    list-style: none;
    float: left;
    width: 100%;
    border-bottom: 1px solid #505050;
    padding: 0px 0px 21px;
    margin: 0px 0px 30px;
}
.widget_twitter_feeds ul li:last-child {
    border-bottom: none;
    margin: 0px;
    padding: 0px;
}
.widget_twitter_feeds ul li > a {
    float: left;
    width: 30px;
    height: 30px;
    position: relative;
    color: #ffffff;
