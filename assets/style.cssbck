/*fixed-btn*/
.fixed-btn {
    position: fixed;
    left: 0px;
    top: 50%;
    z-index: 99;
    transition: all 0.4s ease-in-out;
}
.fixed-button {
    display: inline-block;
    background: #da251c;
    color: #ffffff;
    font-weight: bold;
    padding: 13px 45px 13px 22px;
    position: relative;
    z-index: 1;
}
.fixed-btn.active { left: -9.5%; }
.close-btn {
    background: #92d050;
    color: #ffffff;
    position: absolute;
    right: 0px;
    top: 0px;
    height: 47px;
    line-height: 46px;
    padding: 2px 5px 3px;
    font-size: 18px;
    z-index: 1;
}
.close-btn:before {
    transform: scaleX(-1);
    display: inline-block;
    transition: all 0.4s ease-in-out;
}
.close-btn.active:before { transform: scaleX(1); }
